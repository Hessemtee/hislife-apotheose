{% extends 'base.html.twig' %}

{% block body %}
	<section id="homeIntro" class="parallax first-widget">
		{% for message in app.flashes('danger') %}
				<div class="alert alert-danger message-flash">
					{{ message }}
				</div>
			{% endfor %}
	    <div class="parallax-overlay">
		    <div class="container home-intro-content">
		        <div class="row">
		        	<div class="col-md-12">
		        		<h2>HIS LIFE</h2></br>
		        		<h3 style="font-weight:bold; color:white;">Parents éloignés par le travail, séparés ou autre... soyez toujours au courant de ce qu'il ce passe dans la vie de vos enfants.</h3> </br>
		        		<a href="{{ path('app_register') }}" class="large-button white-color">Inscrivez-vous</a>
		        	</div> <!-- /.col-md-12 -->
		        </div> <!-- /.row -->
		    </div> <!-- /.container -->
	    </div> <!-- /.parallax-overlay -->
	</section> <!-- /#homeIntro -->

	<section class="light-content our-team">
		<div class="container">
			<div class="row">
				<div class="col-md-12">
					<div class="section-header">
