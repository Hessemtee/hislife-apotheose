{% extends 'basedashboard.html.twig' %}

{% block body %}
 
	<section class="light-content our-team">

		<div class="container">
			<div class="row">
				<div class="col-md-12">
					<div class="section-header">
						<h2 class="section-title">Calendrier</h2>
					</div>
					<!-- /.section-header -->
				</div>
				<!-- /.col-md-12 -->
			</div>
			<!-- /.row -->
		</div>
		<!-- /.container -->
<div class="font-poppinsdeux">
			<div class="wrapper wrapper--w780">
				<div class="card card-15">
					<div class="card-heading"></div>
					<div class="card-body">
						<h2 class="title">Ajouter
						</h2>
					{{ form_start(form) }}
					{{ form_row(form.name) }}
					{{ form_row(form.type) }}
					{{ form_row(form.beginAt.date) }}
					{{ form_row(form.beginAt.time) }}
					{{ form_row(form.endAt.date) }}
					{{ form_row(form.endAt.time) }}
					{{ form_row(form.family)}}
	<div class="p-t-10">
	<span style="color: #ffffff;">* Ces champs sont obligatoires</span>
							<button class="large-button white-color" type="submit">Valider</button>
					</div>
					{{ form_end(form) }} 
					</br>
					
						</div>
					</br>

				</div>
			</div>
		</div>
		
	</section>

	
	

{% endblock %}